<div class="container">   
    <h3>Budget</h3>   
    <%# <td><%= @transaction[:user_id] %>
    <td>Income: 1234</td>  
    <%# <td>Budget: <%= @transaction.total %></td>    %>
     
      <div>   
        <%= link_to 'Add Transaction', new_transaction_path %>   
      </div>   
      <br>   
    <div>
      <h1>Expenses</h1>
      <table border="2">   
        <thead>   
          <tr>      
            <th>Name</th>   
            <th>Amount</th>   
            <th>File Upload</th>   
          </tr>   
        </thead>   
        <tbody>   
          <% @transaction.each do |item| %>   
            <% item.expenses.each do |expense| %>   
              <tr>   
                <td><%= expense.name %></td>   
                <td><%= expense.amount %></td>   
                <td><%= expense.file_upload %></td>   
                  <div>   
                  <td>     
                    <%= link_to 'Edit', edit_transaction_path(item) %>   
                    <%= link_to 'Delete', transactions_path(item), method: :delete %>   
                  </div>   
                </td>   
              </tr> 
            <% end %>   
          <% end %>   
        </tbody>   
      </table>   
    </div>
</div>  