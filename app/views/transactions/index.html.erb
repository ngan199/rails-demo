<div class="container">   
    <h3>Transaction</h3>   
     
      <div>   
        <%= link_to 'Add Transaction', transactions_new_path %>   
      </div>   
      <br>   
    <table border="2">   
      <thead>   
        <tr>   
          <th>Name</th>   
          <th>Amount</th>   
          <th>File Upload</th>   
          <th>Total</th>   
          <th>Date Time</th>   
        </tr>   
      </thead>   
      <tbody>   
        <% @transaction.each do |item| %>   
            <% item.expenses.each do |expense| %>  
              <tr>   
                <td><%= expense.name %></td>   
                <td><%= expense.amount %></td>   
                <td><%= expense.file_upload %></td>   
                <td><%= item.total %></td>   
                <td><%= item.created_at %></td>   
                  <div>   
                  <td>     
                    <%= link_to 'Edit', transactions_edit_path(item) %>   
                    <%= link_to 'Delete', transactions_destroy_path(item), method: :delete %>   
                  </div>   
                </td>   
              </tr> 
            <% end %>
        <% end %>   
      </tbody>   
    </table>   
</div>  